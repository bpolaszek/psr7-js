{
  "name": "psr7-js",
  "type": "module",
  "version": "0.3.0",
  "author": "Beno!t POLASZEK",
  "license": "MIT",
  "description": "URI and Query String immutable manipulation, support for bracket syntax.",
  "keywords": [
    "uri",
    "url",
    "query string",
    "search",
    "query",
    "brackets",
    "psr7",
    "psr-7",
    "php"
  ],
  "homepage": "https://github.com/bpolaszek/psr7-js",
  "repository": {
    "type": "git",
    "url": "https://github.com/bpolaszek/psr7-js.git"
  },
  "bugs": {
    "url": "https://github.com/bpolaszek/psr7-js/issues"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest --run",
    "lint": "prettier --check \"(src|tests)/**\" --ignore-unknown",
    "format": "prettier -w \"(src|tests)/**\" --ignore-unknown"
  },
  "devDependencies": {
    "@types/node": "^20.4.2",
    "deepmerge": "^4.3.1",
    "prettier": "^3.0.0",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vite-plugin-dts": "^3.3.1",
    "vitest": "^0.33.0"
  },
  "main": "./dist/psr7-js.umd.cjs",
  "module": "./dist/psr7-js.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/psr7-js.js",
      "require": "./dist/psr7-js.umd.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ]
}
